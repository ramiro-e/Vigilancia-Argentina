<div class="d-flex justify-content-between align-items-center">
    <h2>Productos</h2>
    <div>
        <button class="btn bg-transparent mx-1" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
            <i class="fas fa-arrow-alt-circle-left"></i>
        </button>
        <button class="btn bg-transparent mx-1" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
            <i class="fas fa-arrow-alt-circle-right" ></i>
        </button>
    </div>
</div>
<div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-inner">
        <% if (typeOf(productos) != 'undefined'){ for ( i = 0 , i <= productos.length + 1, i++){if ( i === 0 ){%> 
        
        <div class="carousel-item active">
            <div class="my-0 row row-cols-1 row-cols-sm-2 row-cols-xl-4 g-4">

        <% }else if ((!(i % 4) === 0) && (i < 5)){ %> 
                <div class="col">
                    <div class="card h-100">
                        <img src="/images/products/producto-1621640398775.jpg" class="border-bottom card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title"><strong class="text-primary">CARD TITLE</strong></h5>
                            <p>Codigo: 5992y9nrk2b</p>
                            <div class="d-flex justify-content-between">
                                <span>$50000000 <small>(iva incl.)</small> </span>
                                <span class="text-secondary"><small>EN STOCK</small></span>
                            </div>
                            <a href="#" class="mt-2 w-100 btn btn-outline-primary">VER MAS</a>
                        </div>
                    </div>
                </div>
        <% }else if ((!(i % 4) === 0) && (i > 5)){ %>
            </div>
        </div>
        <div class="carousel-item">
            <div class="my-0 row row-cols-1 row-cols-sm-2 row-cols-xl-4 g-4">
                
                <div class="col">
                    <div class="card h-100">
                        <img src="/images/products/producto-1621640398775.jpg" class="border-bottom card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title"><strong class="text-primary">CARD TITLE</strong></h5>
                            <p>Codigo: 5992y9nrk2b</p>
                            <div class="d-flex justify-content-between">
                                <span>$50000000 <small>(iva incl.)</small> </span>
                                <span class="text-secondary"><small>EN STOCK</small></span>
                            </div>
                            <a href="#" class="mt-2 w-100 btn btn-outline-primary">VER MAS</a>
                        </div>
                    </div>
                </div>
        <% } else if ( i === productos.length + 1){ %>
            </div>
        </div>
        <% }}} %>
    </div> 
</div>


