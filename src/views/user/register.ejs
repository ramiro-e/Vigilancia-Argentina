                            <div class="col-xs-12 col-sm-6">
                                <label for="companyName" class="small text-secondary">Razón Social / Nombre de la Empresa</label>
                                <input name="companyName" class="form-control" type="text" id="companyName" placeholder="Empresa">
                                <small class="d-none">error mesagge</small>
                            </div>
                            <div class="col-xs-12 col-sm-6">
                                <label for="activity" class="small text-secondary">Actividad Principal</label>
                                <input name="activity" class="form-control" type="text" id="activity" placeholder="Actividad">
                                <small class="d-none">error mesagge</small>
                            </div>
                            <div class="col-xs-12 col-sm-6">
                                <label for="cuit" class="small text-secondary">CUIT Nº</label>
                                <input name="cuit" class="form-control" type="text" id="cuit" placeholder="Cuit (sin guiones ni espacios)">
                                <small class="d-none">error mesagge</small>
                            </div>
                            <div class="col-xs-12 col-sm-6">
                                <label for="web" class="small text-secondary">Website</label>
                                <input name="web" class="form-control" type="text" id="web" placeholder="Web">
                                <small class="d-none">error mesagge</small>
                            </div>
                            <div class="col-xs-12 col-sm-6">
                                <label for="phone" class="small text-secondary">Teléfono</label>
                                <input name="phone" class="form-control" type="text" id="phone" placeholder="Teléfono">
                                <small class="d-none">error mesagge</small>
                            </div>
                        </div>
                        
                        <hr class="divider-line">
            
