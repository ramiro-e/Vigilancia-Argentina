                                <div class="row">
                                    <div class="col-xs-12 col-sm-6 w-50">
                                        <label for="country" class="small text-secondary">País</label>
                                        <select name="country" class="form-select" id="country" onchange="envios();">
                                            <option value="">Seleccione el Pais</option>
                                            <option value="1">Argentina</option>
                                        </select>
                                        <small class="d-none">error mesagge</small>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 w-50">
                                        <label for="province" class="small text-secondary">Provincia</label>
                                        <select name="province" class="form-select" id="province">
                                            <option value="">Seleccione la Provincia</option>
                                            <option value="1">Buenos Aires</option>
                                        </select>
                                        <small class="d-none">error mesagge</small>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-6">
