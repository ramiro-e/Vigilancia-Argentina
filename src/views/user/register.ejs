        <section class="container-md">
            <div class="py-4">       
                <% if(typeof(errors) != 'undefined'){%>
                    <div>
                        <ul class="list-unstyled">
                            <% errors.forEach(error => { %>
                                <li class="my-1 px-2 py-1 alert alert-danger"><%= error.msg %></li>
                            <%});%>
                        </ul>
                    </div>
                <%}%>  
                <div class="p-4 bg-white rounded">
